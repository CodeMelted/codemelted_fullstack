{
    "source": {
        "include": [
            "README.md",
            "codemelted_fullstack.js"
        ],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": ["plugins/markdown"],
    "opts": {
        "encoding": "utf8",
        "readme": "./README.md",
        "destination": "docs",
        "recurse": true,
        "verbose": true,
        "template": "/users/DarkT/AppData/Roaming/npm/node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "default_theme": "dark"
        }
    },
    "markdown": {
        "hardwrap": false,
        "idInHeadings": true
    }
}